p { 
  color: #333; 
  font: 400 0.8em verdana,arial,sans-serif;
}
table {
   margin: auto;
}
td {
   color: #333;
   font: 400 0.8em verdana,arial,sans-serif;
   padding: 3px;
}
li {
   color: #333;
   font: 400 0.8em verdana,arial,sans-serif;
   padding: 3px 0 3px 0;
}
h1 {
   color: brown;
   font-size: 200%;
   text-align: center;
}
#login, #contact, #register, #map {
   padding-bottom: 15px;
   width: 80%;
   margin: auto;
   text-align: center;
}

#map {
   width: 100%;
}

#login_form, #contact_form, #register_form {
   width: 382px;
   margin: auto;
   height: 308px;
   padding: 9px;
   background-color: antiquewhite;
   border-radius: 30px;
   transform: translateX(calc(19vw - 180px)) translateY(calc(13vh - 209px))
   box-shadow: rgba(0, 0, 0, 0.5) 15px 15px 80px 10px;
   display: grid;
   grid-template-columns: 1fr;
   grid-gap: 20px;
   align-items: center;
   justify-items: center;
   input{
}
}
#header {
   width: 90%;
   margin: auto;
   height: 80px;
   line-height: 80px;
   vertical-align: center;
   text-align: middle;
}

#body {
   clear: both;
   width: 80%;
   margin: auto;
}
#footer { 
   color: #666;
   font-size: 60%;
   clear: both;
   padding-top: 20px;
   margin: auto;
   text-align: center;
}
#terms {
   text-align: left;
   margin: auto;
}
body {
   height: 100%;
   width: 100%;
   margin: 10px auto 0px auto;
   padding: 0;
   text-align: center;
   font: 400 0.8em verdana,arial,sans-serif;
   line-height: 170%;
   background: aqua;
   color: #333;
}
#openid_url {
  background: url(openid.gif) no-repeat;
  background-color: #fff;
  background-position: 0 50%;
  color: #000;
  padding-left: 18px;
}
#openid-label,#login-label {
  font-size: smaller;
  line-height: 14px;
  vertical-align: middle;
}
.err{
   font-weight: bold;
   color: #000;
   width: 70%;
   margin: 0px auto 10px auto;
}
